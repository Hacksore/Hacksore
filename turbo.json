{
  "globalEnv": [
    "NEXT_PUBLIC_FIREBASE_API_KEY",
    "NEXT_PUBLIC_FIREBASE_DATABASE_URL",
    "NEXT_PUBLIC_FIREBASE_PROJECT_ID",
    "NEXT_PUBLIC_FIREBASE_APP_ID",
    "GITHUB_ACCESS_TOKEN",
    "GITHUB_CLIENT_ID",
    "GITHUB_CLIENT_SECRET",
    "FIREBASE_SA_BASE64",
    "FIREBASE_DATABASE_EMULATOR_HOST",
    "NEXTAUTH_SECRET",
    "DISCORD_TOKEN"
  ],
  "tasks": {
    "start": {
      "persistent": true,
      "cache": false
    },
    "serve": {
      "persistent": true,
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "deploy": {
      "outputs": []
    },
    "clean": {
      "cache": false
    }
  }
}
