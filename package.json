{
  "name": "hacksore",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:web": "concurrently pnpm:dev pnpm:emulator",
    "start": "concurrently pnpm:dev:all pnpm:emulator",
    "emulator": "firebase emulators:start --import private-seed",
    "db:export": "firebase emulators:export private-seed",
    "dev": "turbo start --filter=web",
    "dev:all": "turbo start",
    "build": "turbo build",
    "build:web": "turbo build --filter=web",
    "tsc": "tsc -b --watch",
    "lint": "turbo lint",
    "serve": "turbo serve",
    "format": "npx prettier -w '**/*.{ts,tsx}'",
    "deploy": "firebase deploy && vercel deploy",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.44.0",
    "firebase-tools": "^12.4.3",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "turbo": "^1.10.7",
    "typescript": "^5.1.6",
    "lint": "workspace:*",
    "tsconfig": "workspace:*"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@9.1.0"
}
