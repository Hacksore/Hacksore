{
  "name": "hacksore",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*",
    "packages/*"
  ],
  "scripts": {
    "start:web": "concurrently yarn:dev yarn:emulator",
    "start": "concurrently yarn:dev:all yarn:emulator",
    "emulator": "firebase emulators:start --import private-seed",
    "db:export": "firebase emulators:export private-seed",
    "dev": "turbo run start --filter=web",
    "dev:all": "turbo run start",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=web",
    "tsc": "tsc -b --watch",
    "lint": "turbo run lint",
    "deploy": "firebase deploy && vercel deploy",
    "clean": "turbo clean"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.2",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "firebase-tools": "^11.23.1",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "turbo": "^1.8.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
